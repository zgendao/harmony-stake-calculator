{:source-paths ["src"]

 :dependencies [[binaryage/devtools "0.9.10"]
                [proto-repl "0.3.1"]
                [reagent "0.8.1"]
                [cljs-http "0.1.46"]
                [cljsjs/highcharts "7.0.3-0"]
                [brave/cmc "0.1.0-SNAPSHOT"]]

 :nrepl        {:port 3333}

 :builds
 {:app {:target :browser
        :compiler-options
        {:pseudo-names true
         :pretty-print true}
        :output-dir "public/js"
        :asset-path "/js"

        :modules {:main {:init-fn app.core/main}}

        :devtools
        {:http-root   "public"
         :http-port   3000}}}}
